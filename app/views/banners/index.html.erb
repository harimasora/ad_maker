<p id="notice"><%= notice %></p>

<h1>Listing Banners</h1>

<table>
  <thead>
    <tr>
      <th>Kind</th>
      <th>Description</th>
      <th>State</th>
      <th>Keywords</th>
      <th>Rank</th>
      <th>Image</th>
      <th>Production order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @banners.each do |banner| %>
      <tr>
        <td><%= banner.kind %></td>
        <td><%= banner.description %></td>
        <td><%= banner.state %></td>
        <td><%= banner.keywords %></td>
        <td><%= banner.rank %></td>
        <td><%= banner.image %></td>
        <td><%= banner.production_order %></td>
        <td><%= link_to 'Show', banner %></td>
        <td><%= link_to 'Edit', edit_banner_path(banner) %></td>
        <td><%= link_to 'Destroy', banner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Banner', new_banner_path %>
